# TMDB API Key (required for metadata and search)
# Get your free API key at: https://www.themoviedb.org/settings/api
TMDB_API_KEY=your_api_key_here

# Download settings
PARALLEL_DOWNLOADS=3
SEGMENT_CONCURRENCY=5  # Lower values (3-5) reduce server load and avoid rate limiting
DEFAULT_QUALITY=best
DEFAULT_LANGUAGES=en

# Logging
LOG_LEVEL=INFO
